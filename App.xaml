<Application x:Class="ClipOne.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:tb="http://www.hardcodet.net/taskbar"
             StartupUri="view/MainWindow.xaml" DispatcherUnhandledException="Application_DispatcherUnhandledException"  ShutdownMode="OnExplicitShutdown" >
    <Application.Resources>
        <ContextMenu x:Shared="false" x:Key="SysTrayMenu"  >
            
        </ContextMenu>
        <tb:TaskbarIcon x:Key="Taskbar" 
                        IconSource="ClipOne.ico"   ContextMenu="{StaticResource SysTrayMenu}">
          
        </tb:TaskbarIcon>
     

    </Application.Resources>
</Application>
